{
    "name": "matithyah/yii2-ckeditor-widget",
    "description": "CKEditor widget for Yii2 - fork",
    "type": "yii2-extension",
    "keywords": [
        "2amigos",
        "yii",
        "yii2",
        "yii 2",
        "extension",
        "widget",
        "ckeditor"
    ],
    "homepage": "http://yiiwheels.com/extension/ckeditor-widget",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Matt Williams",
            "email": "itdept@smbc.com.au"
        }
    ],
    "support": {
        "issues": "https://github.com/matithyah/yii2-ckeditor-widget/issues",
        "source": "https://github.com/matithyah/yii2-ckeditor-widget"
    },
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "ckeditor/ckeditor": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\ckeditor\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
